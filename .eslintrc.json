{
  "extends": "@dlghq/eslint-config-dialog/base",
  "rules": {
    "no-sync": 0,
    "no-console": 0,
    "global-require": 0,
    "no-process-env": 0,
    "no-await-in-loop": 0,
    "array-bracket-newline": [2, { "multiline": true, "minItems": 2 }],
    "array-element-newline": [2, { "multiline": true, "minItems": 2 }]
  }
}
